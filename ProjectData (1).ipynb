    "cleveland_plot <- cleveland_training |>\n",
    "    ggplot(aes(x=cholestrol, y=resting_bp)) +\n",
    "    geom_point() +\n",
    "    labs(x=\"Cholestrol Level (mg/dL)\", y=\"Resting Blood Pressure (mmHg)\") + \n",
    "    scale_x_log10()\n",
    "\n",
    "cleveland_plot"
